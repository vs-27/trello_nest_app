{
  "name": "apptest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npx nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm",
    "migration:generate:local": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate --dataSource db/data-source.ts src/migrations/$npm_config_name",
    "migration:generate:prod": "NODE_ENV=production npx typeorm migration:generate --dataSource dist/db/data-source.js dist/migrations/$npm_config_name",
    "migration:run:local": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource db/data-source.ts",
    "migration:run:prod": "NODE_ENV=production node dist/db/data-source.js migration:run",
    "migration:revert:local": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource db/data-source.ts",
    "migration:revert:prod": "NODE_ENV=production node dist/db/data-source.js migration:revert",
    "migration:show:local": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show --dataSource db/data-source.ts",
    "migration:show:prod": "NODE_ENV=production node dist/db/data-source.js migration:show",
    "c": "npx nestjs-command",
    "front-watch": "NODE_ENV=development webpack --config webpack.dev.config.js",
    "front-build": "NODE_ENV=development webpack --config webpack.build.config.js",
    "front-prod": "NODE_ENV=production webpack --config webpack.build.config.js"
  },
  "dependencies": {
